{
  "kind": "collectionType",
  "collectionName": "contracts",
  "info": {
    "singularName": "contract",
    "pluralName": "contracts",
    "displayName": "Contract",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NameVersion": {
      "type": "string",
      "regex": "^[A-Za-z0-9]+@[0-9]+\\.[0-9]+\\.[0-9]+(?:-[A-Za-z0-9]+)?$",
      "required": true,
      "unique": true,
      "default": "<ContractName>@1.0.0"
    },
    "ABI": {
      "type": "json"
    },
    "ByteCode": {
      "type": "json"
    },
    "ReleaseNotes": {
      "type": "text"
    },
    "GitLink": {
      "type": "string"
    },
    "EvmVersion": {
      "type": "string",
      "regex": "\\bhomestead\\b|\\btangerineWhistle\\b|\\bspuriousDragon\\b|\\bbyzantium\\b|\\bconstantinople\\b|\\bpetersburg\\b|\\bistanbul\\b|\\bberlin\\b|\\blondon\\b|\\bparis\\b|\\bshanghai\\b|\\bcancun\\b|\\bprague\\b|\\bosaka\\b"
    },
    "PragmaVersion": {
      "type": "string",
      "regex": "\\^?\\d+\\.\\d+\\.\\d+"
    },
    "optimizerEnabled": {
      "type": "boolean"
    },
    "runs": {
      "type": "integer",
      "min": 0
    },
    "viaIR": {
      "type": "boolean"
    }
  }
}
