      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ],
      "pluginOptions": {}
    },
    "WebSite": {
      "type": "string"
    },
    "Twitter": {
      "type": "string"
    },
    "Telegram": {
      "type": "string"
    },
    "Discord": {
      "type": "string"
    },
    "Color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
