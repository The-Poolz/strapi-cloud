  "collectionName": "poolz_boutiques",
  "info": {
    "singularName": "poolz-boutique",
    "pluralName": "poolz-boutiques",
    "displayName": "PoolzBoutique",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "projectName": {
      "type": "string"
