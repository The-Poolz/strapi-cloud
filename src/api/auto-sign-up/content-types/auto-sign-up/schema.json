{
  "kind": "collectionType",
  "collectionName": "auto_sign_ups",
  "info": {
    "singularName": "auto-sign-up",
    "pluralName": "auto-sign-ups",
    "displayName": "AutoSignUp"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Wallet": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^(0x)?[0-9a-fA-F]{40}$"
    },
    "Amount": {
      "type": "decimal",
      "required": true,
      "max": 0
    },
    "Note": {
      "type": "string",
      "required": true
    }
  }
}
